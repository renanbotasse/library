<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Lista PIs</title>
</head>
<body>
  <h2>Lista de Pontos de Interesse<h2>
  <table border="1">
    <thead>
      <tr>
        <td>Title</td>
        <td>Auuthor</td>
        <td>Status</td>
      </tr>
    </thead>
    <tbody>
      <% books.forEach(function(data) { %>
        <tr>
          <td><%= data.title %></td>
          <td><%= data.author %></td>
          <td><%= data.year %></td>
		  <td><%= data.pages %></td>
        </tr>
     <% }) %>
   </tbody>
   <button><a href="/">Back</a></button>
</body>
</html>